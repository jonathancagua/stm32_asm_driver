@ GPIO_BASE_D 			0x40020C00
@ RCC_BASE 				0x40023800
@ RCCAHB1ENR_OFFSET		0X30
@ GPIOD_EN				1<<3
@ MODER5_OUT			1<<10
@ MODER4_OUT			1<<8
@ GPIOX_MODER_OFFSET	0X00
@ GPIOX_ODR_OFFSET		0X14
@ LED_ON				1<<5


symb1 equ $10;
RCC_BASE				EQU		0x40020C00
RCCAHB1ENR_OFFSET		EQU		0X30
RCCAHB1ENR				EQU		RCC_BASE+RCCAHB1ENR_OFFSET
